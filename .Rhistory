fast_boycei <- function(interval, obs, fit)
{
pi <- sum(.bincode(obs, interval), na.rm = TRUE)/length(obs)
ei <- sum(.bincode(fit, interval, FALSE), na.rm = TRUE)/length(fit)
fi <- pi/ei
return(fi)
}
